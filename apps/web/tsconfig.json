{
    "extends": "tsconfig/nextjs.json",
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/components/*": ["src/components/*"],
            "@/styles/*": ["src/styles/*"],
            "@/lib/*": ["src/lib/*"],
            "@/utils/*": ["src/utils/*"],
            "@/store/*": ["src/store/*"],
            "@/data/*": ["src/data/*"]
        }
    }
}
