{
  "npmClient": "pnpm",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "web#build": {
      "dependsOn": [
        "^build",
        "$NEXT_PUBLIC_SANITY_DATASET",
        "$NEXT_PUBLIC_SANITY_PROJECT_ID"
      ],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
